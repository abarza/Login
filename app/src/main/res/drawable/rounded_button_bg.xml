<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

  <item
      android:gravity="center">
    <shape android:shape="rectangle">

      <solid android:color="@color/white"/>

      <size
          android:width="200dp"
          android:height="50dp"
          />

    </shape>
  </item>

  <item
      android:id="@+id/center_button_rounded"
      android:gravity="right"
      android:right="200dp">
    <shape android:shape="oval">

      <size
          android:width="50dp"
          android:height="50dp"
          />

      <solid android:color="@color/white"/>

    </shape>

  </item>


  <item
      android:id="@+id/right_button_rounded"
      android:left="200dp">
    <shape android:shape="oval">

      <solid android:color="@color/white"/>

      <size
          android:width="50dp"
          android:height="50dp"
          />

    </shape>
  </item>




</layer-list>